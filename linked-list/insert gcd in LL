class Solution {
    public ListNode insertGreatestCommonDivisors(ListNode head) {
        if(head.next == null){
            return head;
        }
        ListNode p1 = head;
        ListNode p2 = head.next;
        while(p2 != null){
            int gcdval = gcd(p1.val,p2.val);
            ListNode res = new ListNode(gcdval);
            p1.next = res;
            res.next = p2;

            p1 = p2;
            p2 = p2.next;
        }
        return head;
    }

    public int gcd(int a,int b){
        while(b != 0){
            int temp = b;
            b = a%b;
            a = temp;
        }
        return a;
    }
}
