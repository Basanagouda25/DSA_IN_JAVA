Brute Force:
class Solution {
    public int smallestDivisor(int[] nums, int threshold) {
        int divisior = 1;
        while(true){
            int sum=0;
            for(int num : nums){
                sum += (num+divisior-1)/divisior;
            }
            if(sum <= threshold){
                return divisior;
            }
            divisior++;
        }
    }
}
