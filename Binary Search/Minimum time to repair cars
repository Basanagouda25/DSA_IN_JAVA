class Solution {
    boolean ispossible(int[] ranks,long mid,int cars)
    {
        long carsfixed = 0;
        for(int rank:ranks){
            carsfixed += Math.sqrt(1.0 * mid/rank);
        }
        return carsfixed>=cars;
    }
    public long repairCars(int[] ranks, int cars) {
        int n = ranks.length;
        long l = 1;
        long r = 1L * cars * cars * Arrays.stream(ranks).max().orElse(Integer.MIN_VALUE);
        long result = -1;
        while(l <= r){
            long mid = l + (r-l)/2;
            if(ispossible(ranks,mid,cars)){
                result = mid;
                r = mid-1;
            }else{
                l = mid + 1;
            }
        }
        return result;
    }
}
