class Solution {
    public int lastRemaining(int n) {
        int head = 1;
        int step = 1;
        int size = n;
        boolean lefttoright = true;

        while(size > 1){
            if(size % 2 != 0 || lefttoright){
                head += step;
            }
            step *=2;
            size /= 2;
            lefttoright = !lefttoright;
        }
        return head;
    }
}
